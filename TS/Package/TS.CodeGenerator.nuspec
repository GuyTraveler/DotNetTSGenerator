<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>TS.CodeGenerator</id>
        <version>1.0.0.11</version>
        <title>.Net TS Generator</title>
        <authors>Max Fridberg</authors>
        <owners>Max Fridberg</owners>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>Package comprised of dll and ps1 to run as a post build,
      PS1 will generate a d.ts and allow you to modify where to place it.
      All you need to do is run it as a post-build event.
      Note this is NOT a TT (template file).</description>
        <copyright>Copyright 2014</copyright>
        <tags>TypeScript</tags>
        <projectUrl>https://github.com/KnowledgeLakegithub/.NetTSGenerator</projectUrl>
    </metadata>
    <files>
        <file src="Generate_dts.ps1" target="content\" />
        <file src="..\TS.CodeGenerator\bin\Debug\TS.CodeGenerator.dll" target="tools\TS.CodeGenerator.dll" />
        <file src="..\TS.CodeGenerator\bin\Debug\TS.CodeGenerator.pdb" target="tools\TS.CodeGenerator.pdb" />
        <file src="install.ps1" target="tools\install.ps1" />
    </files>
</package>